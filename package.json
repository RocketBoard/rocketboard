{
  "name": "rocketboard",
  "version": "0.1.0",
  "devDependencies": {
    "generator-flight": "~0.8.0",
    "gulp-livereload": "~0.2.0",
    "jasmine-node": "^1.14.5",
    "karma": "~0.12.0",
    "karma-chrome-launcher": "~0.1.0",
    "karma-cli": "~0.0.4",
    "karma-coverage": "^0.2.7",
    "karma-firefox-launcher": "~0.1.0",
    "karma-jasmine": "~0.2.0",
    "karma-requirejs": "~0.2.1",
    "node-static": "~0.7.3",
    "requirejs": "~2.1.11",
    "tiny-lr": "~0.0.5"
  },
  "engines": {
    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RocketBoard/rocketboard/"
  },
  "scripts": {
    "build": "gulp rjs",
    "livereload": "gulp livereload",
    "start": "node app.js",
    "test": "npm run client-test; npm run server-test",
    "client-test": "karma start --single-run",
    "server-test": "jasmine-node test/config",
    "functional-test": "mvn -f test/spec/functionalTest/rocketboard/pom.xml -Dtest=rocketboard.* test",
    "watch": "npm run start > server.log & npm run livereload",
    "watch-test": "karma start",
    "postinstall": "bower install; gulp rjs"
  },
  "dependencies": {
    "body-parser": "^1.10.0",
    "bower": "^1.3.9",
    "express": "^4.8.7",
    "istanbul": "^0.3.2",
    "gulp": "~3",
    "gulp-requirejs": "^0.1.3",
    "gulp-uglify": "^1.0.2",
    "node-sass": "~0.9.3",
    "xmlhttprequest": "^1.6.0",
    "redis": "^0.12.1",
    "hiredis": "^0.1.17"
  }
}
